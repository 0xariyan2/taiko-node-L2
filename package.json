{
	"name": "taiko",
	"version": "0.0.3",
	"description": "An easy to use wrapper over Google Chrome's Puppeteer library.",
	"main": "taiko.js",
	"bin": {
		"taiko": "repl.js"
	},
	"scripts": {
		"format": "./node_modules/.bin/js-beautify *.js -r -b collapse,preserve-inline && npm run lint",
		"lint": "./node_modules/.bin/eslint *.js",
		"test": "npm run lint"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/getgauge/taiko"
	},
	"keywords": [
		"headless",
		"headless-chrome",
		"headless-testing",
		"headless-browser",
		"puppeteer"
	],
	"author": "getgauge",
	"license": "MIT",
	"dependencies": {
		"babylon": "^6.18.0",
		"figlet": "^1.2.0",
		"puppeteer": "^0.12.0",
		"recast": "^0.12.8",
		"repl.history": "^0.1.4"
	},
	"devDependencies": {
		"eslint": "^4.10.0",
		"js-beautify": "^1.7.4"
	}
}
